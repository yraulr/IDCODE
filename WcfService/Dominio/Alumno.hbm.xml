<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="WcfService"
                   namespace="WcfService.Dominio"
                   default-lazy="false">
  <class name="Alumno" table="tb_alumno">
    <id name="cd_alumno" column="cd_alumno">
      <generator class="increment" />
    </id>
    <property name="ds_identificador_alumno" column="ds_identificador_alumno" />
    <property name="ds_nombre" column="ds_nombre" />
    <property name="ds_apellido" column="ds_apellido" />
    <property name="cd_padre" column="cd_padre" />
    <property name="cd_pago" column="cd_pago" />
  </class>
</hibernate-mapping>